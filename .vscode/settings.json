{
  "[css]": {
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.suggest.insertMode": "replace"
  },
  "terminal.integrated.env.windows": {
    "PATH": "./node_modules/.bin;libs/Windows/x64/node;libs/Windows/x64/python2.7;libs/Windows/x64/utility"
  },
  "terminal.integrated.env.linux": {
    "PATH": "./node_modules/.bin;libs/Windows/x64/node;libs/Windows/x64/python2.7;libs/Windows/x64/utility"
  },
  "terminal.integrated.env.osx": {
    "PATH": "./node_modules/.bin;libs/Windows/x64/node;libs/Windows/x64/python2.7;libs/Windows/x64/utility"
  },
  "intelephense.format.braces": "psr12",
  "php-cs-fixer.autoFixByBracket": true,
  "php-cs-fixer.onsave": false,
  "php-cs-fixer.autoFixBySemicolon": false,
  "php-cs-fixer.lastDownload": 1,
  "prettier.tabWidth": 2,
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.formatOnPaste": false,
  "typescript.tsserver.maxTsServerMemory": 1500,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.renameOnType": true,
  "editor.wordWrap": "on",
  "eslint.packageManager": "npm",
  "explorer.autoReveal": true,
  "files.associations": {
    "*.extension": "language"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 18000,
  "files.encoding": "utf8",
  "files.exclude": {
    "out": false,
    ".vscode": false
  },
  "files.maxMemoryForLargeFilesMB": 100,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/log/**": true,
    "**/node_modules/**": true,
    "**/tmp/**": true,
    "**/vendor/**": true,
    "**/src/**": true,
    "**/assets/**": true,
    "**/libs/**": true,
    "**/views/**": true
  },
  //"typescript.tsdk": "./node_modules/typescript/lib",
  "npm.enableScriptExplorer": false,
  "npm.enableRunFromFolder": false,
  "typescript.tsserver.watchOptions": {
    "fallbackPolling": "dynamicPriorityPolling",
    "synchronousWatchDirectory": true,
    "watchDirectory": "dynamicPriorityPolling",
    "watchFile": "dynamicPriorityPolling"
  },
  "npm.exclude": "**libs**",
  "typescript.tsserver.useSeparateSyntaxServer": true,
  "typescript.suggest.autoImports": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.autofetchPeriod": 18000,
  "git.autorefresh": true,
  "git.countBadge": "all",
  "git.enableSmartCommit": true,
  "git.enableStatusBarSync": true,
  "git.enabled": true,
  "git.smartCommitChanges": "all",
  "git.supportCancellation": false,
  "github.gitAuthentication": true,
  "intelephense.maxMemory": 1500,
  "intelephense.telemetry.enabled": false,
  "intelephense.trace.server": "off",
  "markdown.extension.list.indentationSize": "adaptive",
  "markdown.preview.scrollEditorWithPreview": true,
  "npm.packageManager": "npm",
  "php-cs-fixer.executablePath": "D:\\dimaslanjaka.github.io\\php-yt-api\\libs\\bin\\php-cs-fixer\\php-cs-fixer.phar",
  "php-cs-fixer.executablePathWindows": "D:\\dimaslanjaka.github.io\\php-yt-api\\libs\\bin\\php-cs-fixer\\php-cs-fixer.bat",
  "php-docblocker.author": {
    "name": "Dimas Lanjaka",
    "email": "dimaslanjaka@gmail.com"
  },
  "prettier.packageManager": "npm",
  "python.pythonPath": "B:\\Users\\L3n4r0x\\AppData\\Local\\Programs\\Python\\Python38\\python.exe",
  "search.exclude": {
    "out": true,
    ".vscode": true
  },
  "task.problemMatchers.neverPrompt": true,
  "terminal.integrated.environmentChangesIndicator": "on",
  "typescript.check.npmIsInstalled": false,
  "typescript.tsc.autoDetect": "off",
  "php-cs-fixer.formatHtml": true,
  "php-cs-fixer.rules": "@PSR2",
  "php-cs-fixer.config": ".php_cs;.php_cs.dist",
  "npm.autoDetect": "off",
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "jake.autoDetect": "off",
  "task.autoDetect": "off",
  "sync.autoDownload": false,
  "sync.forceUpload": true,
  "debug.javascript.terminalOptions": {
    "autoAttachChildProcesses": true
  },
  "typescript.disableAutomaticTypeAcquisition": true,
  "npm.runSilent": true,
  "prettier.resolveGlobalModules": true,
  "editor.minimap.size": "fit",
  "sqltools.historySize": 30,
  "extensions.ignoreRecommendations": true,
  "sqltools.disableReleaseNotifications": true,
  "search.useIgnoreFiles": false,
  "problems.showCurrentInStatus": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "npm.fetchOnlinePackageInfo": false,
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.locale": "en",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": false,
  "prettier.useEditorConfig": true,
  "git.ignoreLimitWarning": true
}